<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.labuser.caretojoin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartPageActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".RegisterUserActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".StartPageActivity" />
        </activity>

        <activity android:name=".PostDetailActivity">
            <!--<meta-data-->
            <!--android:name="android.support.PARENT_ACTIVITY"-->
            <!--android:value=".MapActivity"/>-->
        </activity>

        <activity android:name=".MapActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".StartPageActivity"/>
        </activity>

        <activity android:name=".CreatePostActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MapActivity"/>
        </activity>
    </application>

</manifest>